      },
      "exposed" : false
    },
    "buildContent" : {
      "type" : "expression",
      "value" : [
        "let output = [];",
        "",
        "if($data.objSolicitudDeAdmision && $data.objSolicitudApoyo){",
        "    let object = {",
        "        \"folio\": $data.objSolicitudApoyo[0].caseId,",
        "        \"periodo\": $data.objSolicitudDeAdmision[0].catPeriodo.descripcion,",
        "        \"nombre\": buildName($data.objSolicitudDeAdmision[0]),",
        "        // \"estado\": $data.objSolicitudDeAdmision[0].catCampusEstudio.estado.descripcion",
        "        \"estado\": $data.objSolicitudApoyo[$data.objSolicitudApoyo.length - 1].estatusSolicitud",
        "    }    ",
        "    ",
        "    output.push(object);",
        "}",
        "",
        "",
        "",
        "return output;",
        "",
        "function buildName(objSolicitud){",
        "    let output = objSolicitud.primerNombre;",
        "    if(objSolicitud.segundoNombre){",
        "        output += \" \" + objSolicitud.segundoNombre;",
        "    }",
        "    output += \" \" + objSolicitud.apellidoPaterno;",
        "    if(objSolicitud.apellidoMaterno){",
        "        output += \" \" + objSolicitud.apellidoMaterno;",
        "    }",
        "",
        "    return output;",
        "}"
      ],
      "exposed" : false
