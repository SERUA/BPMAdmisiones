      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.lstCatEscolaridad ){",
        "\treturn {",
        "\t\t//map lstCatEscolaridad variable to expected task contract input",
        "\t\tlstCatEscolaridadInput: $data.lstCatEscolaridad.map( it => ({",
        "\t\t\tpersistenceId_string: it.persistenceId_string !== undefined ? it.persistenceId_string : null,",
        "\t\t\tclave: it.clave !== undefined ? it.clave : null,",
        "\t\t\tdescripcion: it.descripcion !== undefined ? it.descripcion : null,",
        "\t\t\tfechaCreacion: it.fechaCreacion !== undefined ? it.fechaCreacion : null,",
        "\t\t\tusuarioCreacion: it.usuarioCreacion !== undefined ? it.usuarioCreacion : null,",
        "\t\t\tisEliminado: it.isEliminado !== undefined ? it.isEliminado : null",
        "\t\t}))",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
