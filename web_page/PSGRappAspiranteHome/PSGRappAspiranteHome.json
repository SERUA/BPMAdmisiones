        "    }",
        "    ",
        "    // Buscar el ultimo case en los casos abiertos",
        "    $data.openCases.forEach(item => {",
        "        const itemStartDate = new Date(item.start);",
        "        if (itemStartDate > lastCaseStartDate) {",
        "            lastCase = item;",
        "            lastCaseStartDate = itemStartDate;",
        "        }",
        "    });",
        "    // Buscar el ultimo case en los casos archivados",
        "    $data.archivedCases.forEach(item => {",
        "        const itemStartDate = new Date(item.start);",
        "        if (itemStartDate > lastCaseStartDate) {",
        "            lastCase = item;",
        "            lastCaseStartDate = itemStartDate;",
        "        }",
        "    });",
        "    ",
        "    return lastCase;",
        "}",
        "else return null;"
