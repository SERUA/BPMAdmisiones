        "};",
        "",
        "function getCatalogo(_objeto, _nombre){",
        "    if(!_objeto){",
        "        return null;",
        "    } else if(_objeto){",
        "        if(_objeto[_nombre]){",
        "            return {persistenceId_string: _objeto[_nombre].persistenceId_string}",
