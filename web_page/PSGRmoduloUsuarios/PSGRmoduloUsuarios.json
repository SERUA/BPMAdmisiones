        "            //resultado.push({...$data.lstCampusRole[index],\"group_id\":$data.lstCampusByUser[x].id});\r",
        "            let r2 = [];\r",
        "            r2.push({...$data.lstCampusRole[index],\"group_id\":$data.lstCampusByUser[x].id});\r",
        "            let i = 0;\r",
        "            resultado.forEach(value =>{\r",
        "                if(value.valor == r2[0].valor ){\r",
        "                   i++;\r",
